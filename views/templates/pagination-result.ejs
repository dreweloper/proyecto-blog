<% if (pagination.totalPages > 1) { %>

    <div id="paginate" class="paginate">

        <% if (pagination.prevPage != null) { %>

            <a href="https://proyecto-blog-p25u.onrender.com/search?search=<%= search %>&page=<%= pagination.prevPage %>" id="btn-prev" data-page="<%= pagination.prevPage %>">
            
                <i class="fa-solid fa-arrow-left"></i>

            </a>
            
        <% } %>

        <p> <%= pagination.page %> </p>

        <% if (pagination.nextPage != null) { %>

            <a href="https://proyecto-blog-p25u.onrender.com/search?search=<%= search %>&page=<%= pagination.nextPage %>" id="btn-next" data-page="<%= pagination.nextPage %>">
            
                <i class="fa-solid fa-arrow-right"></i>
            
            </a>
            
        <% } %>

    </div>
    
<% } %>